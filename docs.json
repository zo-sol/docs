{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "IQLabs SDK",
  "colors": {
    "primary": "#00FF00",
    "light": "#00FF00",
    "dark": "#00CC00",
    "background": {
      "light": "#0a0a0a",
      "dark": "#000000"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "advanced"
            ]
          }
        ]
      },
      {
        "tab": "한국어",
        "groups": [
          {
            "group": "시작하기",
            "pages": [
              "ko/index",
              "ko/quickstart",
              "ko/advanced"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "X (Twitter)",
          "href": "https://x.com/IQLabsOfficial",
          "icon": "x-twitter"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/IQCoreTeam/IQSdkUsageExampleCliTool",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "X (Twitter)",
        "href": "https://x.com/IQLabsOfficial"
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/IQLabsOfficial",
      "github": "https://github.com/IQCoreTeam/IQSdkUsageExampleCliTool"
    }
  }
}
